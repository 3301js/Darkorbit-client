{
  "version": "0.2.0",
  "configurations": [{
      "name": "startDev",
      "request": "launch",
      "type": "node",
      "args": [".", "--dev"],
      "program": "${workspaceFolder}/node_modules/.bin/electron.cmd"
    },
    {
      "name": "Depurar el Proceso Principal",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "args": [".", "--dev"],
      "outputCapture": "std"
    }
  ]
}